<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{ISO8601} [%-5level] [%thread] [%logger] %msg%n</pattern>
        </encoder>
    </appender>
    <root level="${LOG_LEVEL:-info}">
        <appender-ref ref="console"/>
    </root>
</configuration>
